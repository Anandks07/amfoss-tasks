#include <iostream>
using namespace std;

int main() {
	int T,N,i,k,j;
	cin>>T;
	for(i=0;i<T;i++)
	{
	    cin>>N;
	    k=0;
	    for(j=0;j<N;j++)
	 {
	    if((j%5==0)&&(j%3==0))
	    {
	        k=k+j;
	    }
	    if((j%5==0)&&(j%15!=0))
	    {
	       k=k+j;
	    }
	    if((j%3==0)&&(j%15!=0))
	    {
	       k=k+j;
	    }
	 }
	 cout<<k<<"\n";
	} 
	    
	    
	return 0;
}
