#include <iostream>
#include <cstring>
using namespace std;

int main() {
	int a[100];
	int i,j,n;
	j=1;
	n=0;
	for(i=0;i<100;i++)
	{
	  cin>>a[i];
	  n++;
	}
	for(i=1;i<100;i++)
	{
	    if(a[i]==a[i-1])
	   {
	     j++;
	          
	   }       
	}
	if(j>=7)
	{
	    cout<<"YES";
	}
	if(j<7)
	{
	    cout<<"NO";
	}
	
	return 0;
}
